<h1>New beer</h1>


<%= form_for(@beer) do |f| %>
    name: <%= f.text_field :name %>
    style: <%= f.select :style, options_for_select(@styles) %>
    <%= f.select :brewery_id, options_from_collection_for_select(@breweries, :id, :to_s) %>
    <%= f.submit %>
<% end %>

<%= link_to 'Back', beers_path %>
